ry.scss","name":null,"original":{"line":121,"column":6},"generated":{"line":143,"column":2}},{"source":"css\\sass\\components\\_order-summary.scss","name":null,"original":{"line":121,"column":6},"generated":{"line":143,"column":34}}],"sources":{"css\\sass\\base\\_base.scss":"@use 'abstracts/variables' as *;\r\n\r\n*,\r\n*::before,\r\n*::after {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\nhtml {\r\n  font-size: 62.5%;\r\n  font-family: \"Red Hat Display\", sans-serif;\r\n\r\n  @media only screen and (max-width: 600px) {\r\n    font-size: 50%;\r\n  }\r\n}\r\n\r\nbody {\r\n  background-image: url(/images/pattern-background-desktop.svg);\r\n  background-size: contain;\r\n  background-repeat: no-repeat;\r\n  background-color: $clr-primary-blue;\r\n\r\n  min-height: 100vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n\r\n  @media only screen and (max-width: 768px) {\r\n    background-image: url(/images/pattern-background-mobile.svg);\r\n    padding: 2rem;\r\n  }\r\n}\r\n","css\\sass\\abstracts\\_variables.scss":"$clr-primary-blue: hsl(225, 100%, 94%);\r\n$clr-accent-blue: hsl(245, 75%, 52%);\r\n\r\n$clr-neutral-blue: hsl(225, 100%, 98%);\r\n$clr-desat-blue: hsl(224, 23%, 55%);\r\n$clr-dark-blue: hsl(223, 47%, 23%);\r\n","css\\sass\\components\\_order-summary.scss":"@use 'abstracts/variables' as *;\r\n\r\n.order-container {\r\n  width: 450px;\r\n  max-height: 700px;\r\n  //   max-width: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  border-radius: 1.5rem;\r\n  overflow: hidden;\r\n  background-color: #fff;\r\n  box-shadow: 0 2rem 1.6rem rgba($clr-accent-blue, 0.1);\r\n  margin: auto;\r\n}\r\n\r\n.img-container {\r\n  height: 250px;\r\n  background-image: url(/images/illustration-hero.svg);\r\n  background-size: cover;\r\n  background-repeat: no-repeat;\r\n  background-position: center;\r\n  width: 100%;\r\n\r\n  @media only screen and (max-width: 768px) {\r\n    height: 200px;\r\n  }\r\n}\r\n\r\n.order {\r\n  background-color: #fff;\r\n  display: flex;\r\n  gap: 3rem;\r\n  flex-direction: column;\r\n  text-align: center;\r\n  padding: 5rem;\r\n  font-size: 1.6rem;\r\n\r\n  &__icon {\r\n    width: 6rem;\r\n    height: auto;\r\n  }\r\n\r\n  &__title {\r\n    font-size: 2.5rem;\r\n    font-weight: 900;\r\n  }\r\n\r\n  &__text {\r\n    font-weight: 300;\r\n    color: $clr-desat-blue;\r\n  }\r\n\r\n  &__plan {\r\n    background-color: $clr-neutral-blue;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    padding: 2rem;\r\n    border-radius: 1rem;\r\n    transition: all 0.2s;\r\n  }\r\n\r\n  &__plan-left {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 1rem;\r\n  }\r\n\r\n  &__plan-title {\r\n    font-size: 1.6rem;\r\n\r\n    & span {\r\n      display: block;\r\n      font-weight: 300;\r\n      color: $clr-desat-blue;\r\n    }\r\n  }\r\n\r\n  &__change {\r\n    font-weight: 600;\r\n    color: $clr-accent-blue;\r\n    border: none;\r\n    background-color: transparent;\r\n\r\n    &:hover {\r\n      color: black;\r\n      transform: scale(1.1);\r\n    }\r\n  }\r\n\r\n  &__payment {\r\n    background-color: $clr-accent-blue;\r\n    border: none;\r\n    padding: 1.2rem;\r\n    color: #fff;\r\n    font-weight: 600;\r\n    border-radius: 0.5rem;\r\n    box-shadow: 0 2rem 1.6rem rgba($clr-accent-blue, 0.2);\r\n    margin-bottom: 1.5rem;\r\n    cursor: pointer;\r\n    transition: all 0.3s;\r\n\r\n    &:hover {\r\n      transform: translateY(-4px);\r\n      background-color: rgba($clr-accent-blue, 0.8);\r\n    }\r\n  }\r\n\r\n  &__cancel {\r\n    align-self: center;\r\n    border: none;\r\n    width: max-content;\r\n    background-color: transparent;\r\n    color: $clr-desat-blue;\r\n    font-weight: 600;\r\n    cursor: pointer;\r\n    transition: all 0.2s;\r\n    border-bottom: 2px solid transparent;\r\n\r\n    &:hover {\r\n      border-bottom: 2px solid $clr-accent-blue;\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"ec6d11dc14fcfe18e18ba3e74dd50d0d","cacheData":{"env":{}}}